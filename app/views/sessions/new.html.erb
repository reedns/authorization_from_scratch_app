<h1>Sign in</h1>

<%=form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="field">
    <%= label_tag :pasword %>
    <%= password_field_tag :password %>
  </div>

  <div class="action"><%= submit_tag %></div>
<% end %>
